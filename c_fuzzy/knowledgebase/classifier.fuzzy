/* Test classifier */

CLASS Rectangle
	VARIABLES
		Xmin;
		Xmax;
		Ymin;
		Ymax;
	END_VARIABLES
END_CLASS

CLASS Door extends Rectangle IMPORTANT
	
	CONSTANTS
		Height = 220;
	END_CONSTANTS
	
	FormFactor is Narrow;
	Handle.x is Lateral between(Xmin, Xmax); 
	Handle.y is Centered between(Ymin, Ymax);

END_CLASS

CLASS ConcentricCircle
	VARIABLES
		x;
		y;
	END_VARIABLES
	
	ConcentricCircle.x match x is Good;
	ConcentricCircle.y match y is Perfect;	

END_CLASS
