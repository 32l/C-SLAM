/* Test classifier */

CLASS Rectangle
	VARIABLES
		Xmin;
		Xmax;
		Ymin;
		Ymax;
	END_VARIABLES
END_CLASS

CLASS Door extends Rectangle IMPORTANT
	
	CONSTANTS
		Height = 220;
	END_CONSTANTS
	
	FormFactor is Narrow;
	Handle.X is Lateral between(Xmin, Xmax); 
	Handle.Y is Centered between(Ymin, Ymax);

END_CLASS

